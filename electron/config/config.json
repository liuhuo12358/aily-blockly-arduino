{
  "lang": "zh_CN",
  "theme": "default",
  "font": "default",
  "appdata_path": {
    "win32": "%HOMEPATH%\\AppData\\Local\\aily-project",
    "linux": "~/.config/aily-project",
    "darwin": "~/Library/aily-project"
  },
  "project_path": "%HOMEPATH%\\Documents\\aily-project",
  "region": "cn",
  "regions": {
    "cn": {
      "name": "China",
      "enabled": true,
      "api_server": "http://127.0.0.1:8080",
      "npm_registry": "https://registry.diandeng.tech",
      "resource": "https://blockly.diandeng.tech",
      "updater": "https://dl.diandeng.tech/blockly"
    },
    "eu": {
      "name": "Europe",
      "enabled": true,
      "api_server": "https://api.aily.pro",
      "npm_registry": "https://registry.aily.pro",
      "resource": "https://rs1.aily.pro",
      "updater": "https://dl.aily.pro/blockly"
    },
    "localhost": {
      "name": "Local",
      "enabled": true,
      "api_server": "https://aapi.diandeng.tech",
      "npm_registry": "http://localhost:4873",
      "resource": "http://localhost:4874",
      "updater": ""
    }
  },
  "compile": {
    "verbose": true,
    "warnings": "error"
  },
  "upload": {
    "verbose": true,
    "warnings": "error"
  },
  "platform": "win32",
  "devmode": false,
  "blockly": {
    "renderer": "thrasos"
  }
}